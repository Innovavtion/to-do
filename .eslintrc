{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-console": "warn",
    "react/prop-types": "off",
    "max-classes-per-file": "off",
    "no-useless-concat": "off"
  },
  "plugins": ["react", "prettier"]
}
